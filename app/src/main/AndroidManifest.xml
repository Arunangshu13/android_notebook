<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.notebook.main">

    <application
        android:allowBackup="true"
        android:icon="@drawable/display_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/display_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Notebook">
        <activity
            android:name=".UpdateProfile"
            android:exported="true" />
        <activity
            android:name=".OneTimeSignup"
            android:exported="true" />
        <activity
            android:name=".SplashScreen"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditNote"
            android:exported="true" />
        <activity
            android:name=".NoteManager"
            android:exported="true" />
        <activity
            android:name=".ViewNotes"
            android:exported="true" />
        <activity
            android:name=".CreateNote"
            android:exported="true" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity"
            android:exported="true" >

        </activity>
    </application>

</manifest>